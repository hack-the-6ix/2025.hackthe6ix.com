  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.9",
    "classnames": "^2.5.1",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.86.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.13.12",
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.1",
    "sass-embedded": "^1.83.4",
    "tailwindcss": "^4.0.9",
    "typescript": "^5",
    "wrangler": "^3.22.3"
  }
