{
  "name": "2025.hackthe6ix.com",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8",
    "node": ">=16"
  },
  "engineStrict": false,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npx wrangler pages dev .vercel/output/static",
